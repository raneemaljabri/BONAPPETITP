      <TextField fx:id="items" layoutX="19.0" layoutY="57.0" prefHeight="25.0" prefWidth="186.0" promptText="ex:Um Ali-Sweet" stylesheets="@styleSheet.css" />
      <TextArea fx:id="noteTextArea" layoutX="18.0" layoutY="120.0" prefHeight="56.0" prefWidth="283.0" promptText="Enter your notes " stylesheets="@styleSheet.css" />
