      <Button fx:id="backAll" layoutX="236.0" layoutY="59.0" mnemonicParsing="false" onAction="#switchToBack" prefHeight="15.0" prefWidth="60.0" style="-fx-border-color: #000000;" stylesheets="@styleSheet.css" text="Back" textAlignment="CENTER">
