<AnchorPane id="AnchorPane" prefHeight="589.0" prefWidth="326.0" style="-fx-background-color: F6F8F4;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bonappetitp.FXMLDocumentController">
